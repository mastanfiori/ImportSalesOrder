<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog id="AppLogDialog" title="{i18n>TITLE_APP_LOG}" contentHeight="45%" contentWidth="60%" draggable="true" resizable="true">
		<Page id="ApplicationLogPage" showHeader="false">
			<core:ComponentContainer id="LogMessagesControlContainer" height="100%" />
		</Page>
		<endButton>
			<Button id="logCloseBtn" text="{i18n>CLOSE_LOG_VIEW}" press="onLogCloseBtnPressed" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>
